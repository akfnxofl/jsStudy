var month; // month 변수 선언
date = 25; // date 변수 묵시적 선언
var date = 25; // 변수 선언과 동시에 초기화
month = 12; // 변수 초기화

var month, date; // 여러 변수를 한 번에 선언
var hours = 7, minutes = 15; // 여러 변수를 한 번에 선언 및 초기화
month = 10, date = 5; // 여러 변수를 한 번에 초기화

/*
js 내에서 변수의 타입은 정해져 있지 않고, 동일한 변수에 다른 타입의 값을
다시 대입할 수 있다. 다만, 한 번 선언된 변수를 재선언 하는 것은 불가능하다.
*/